{
  "manifest_version": 3,
  "name": "YouTube Video Button",
  "version": "1.0",
  "description": "Add a button to YouTube videos",
  "permissions": ["scripting", "activeTab"],
  "host_permissions": ["*://www.youtube.com/*"],
  "content_scripts": [
    {
      "matches": ["*://www.youtube.com/*"],
      "js": ["test.js"],
      "run_at": "document_end"
    }
  ]
}